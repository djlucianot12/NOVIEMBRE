{
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/projects-data/(.*)",
      "dest": "/projects-data/$1"
    },
    {
      "src": "/.*\\.(js|css|png|jpg|jpeg|gif|svg|json|ico|txt|woff|woff2)$",
      "dest": "/$0"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}